QComboBox
{
    min-width: 130px;
    border: 1px solid #d4d4d4;
    background-color: #ffffff;
    color: #000000;
    padding-left: 5px;
    padding-right: 20px;
    padding-top: 3px;
    padding-bottom: 3px;
}

QComboBox QAbstractItemView {
    background-color: #ffffff;
    border: 1px solid #d4d4d4;
    color: #000000;
    outline: 0px; /*Go to dotted line*/
}
QComboBox QAbstractItemView::item {
    padding-left: 5px;
    padding-right: 15px;
    padding-top: 3px;
    padding-bottom: 3px;
}
QComboBox QAbstractItemView::item:selected{
    background-color:#e1e1e1;
    color: #000000;
    border: none;
}

