QToolButton
{
    background-color: #f8f8f8;
    border: 1px solid #f8f8f8;
    margin: 1px;
    padding: 2px;
}

QToolTip
{
    color: black;
    background-color: #ffffff;
    border: 1px solid #616161;
}

QToolButton
{
    padding: 0px;
    margin: 1px;
    min-width: 20;
    min-height: 20px;
}

