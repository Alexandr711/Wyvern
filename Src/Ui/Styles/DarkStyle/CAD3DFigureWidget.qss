QComboBox
{
    min-width: 130px;
    border: 1px solid #454545;
    background-color: #1f1f1f;
    color: #ffffff;
    padding-left: 5px;
    padding-right: 20px;
    padding-top: 3px;
    padding-bottom: 3px;  
}

QComboBox QAbstractItemView{
    background-color: #1f1f1f;
    border: 1px solid #454545;
    color: #ffffff;
    outline: 0px; /*Go to dotted line*/
}
QComboBox QAbstractItemView::item {
    background-color: transparent;
    padding-left: 5px;
    padding-right: 15px;
    padding-top: 3px;
    padding-bottom: 3px;
}

QComboBox QAbstractItemView::item:hover{
    background-color:#454545;
    color: #ffffff;
    border: none;
}

QComboBox QAbstractItemView::item:selected{
    background-color:#454545; 
    color: #ffffff;
    border: none;
}