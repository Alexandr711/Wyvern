QToolButton
{
    background-color: #181818;
    border: 1px solid #181818;
    margin: 1px;
    padding: 2px;
    min-width: 20;
    min-height: 20px;
}


QToolTip
{
    color: white;
    background-color: #1f1f1f;
    border: 1px solid #181818;
}

QLabel
{
    image: url(":/Images/Src/Ui/Icons/Dark3DToolsIcons/Separator.png");
    margin-top: 0px;
    margin-bottom: 0px;
    padding-top: 0px;
    padding-bottom: 0px;
}


